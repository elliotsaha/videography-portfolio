{
  "name": "film-portfolio",
  "description": "A Film Portfolio made with NextJS",
  "version": "1.0.0",
  "private": true,
  "author": "Heril Saha <herilsaha06@gmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "test": "jest --env=jsdom ---passWithNoTests --detectOpenHandles --runInBand --ci --reporters=default --reporters=jest-junit --coverage --coverageDirectory=coverage",
    "test:watch": "jest --env=jsdom ---passWithNoTests --watchAll",
    "test:update": "jest --env=jsdom --updateSnapshot",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "commit": "cz",
    "gen": "ts-node ./src/scripts/generateComponent.ts"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "@emotion/react": "11.9.3",
    "@emotion/styled": "11.9.3",
    "@mui/icons-material": "5.8.4",
    "@mui/material": "5.8.7",
    "@styled-system/css": "5.1.5",
    "@types/enzyme": "3.10.12",
    "@types/jest": "28.1.4",
    "@types/styled-components": "5.1.25",
    "ansi-regex": "6.0.1",
    "axios": "0.27.2",
    "braces": "3.0.2",
    "enzyme": "3.11.0",
    "follow-redirects": "1.15.1",
    "glob-parent": "6.0.2",
    "jest": "28.1.2",
    "nanoid": "4.0.0",
    "next": "12.2.0",
    "next-seo": "5.4.0",
    "polished": "4.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hot-toast": "2.2.0",
    "react-icons": "4.4.0",
    "set-value": "4.1.0",
    "styled-breakpoints": "11.1.1",
    "styled-components": "5.3.5",
    "styled-system": "5.1.5",
    "ts-jest": "28.0.5"
  },
  "_moduleAliases": {
    "@": "./src",
    "@/public": "./public"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@netlify/plugin-nextjs": "4.29.2",
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/styled-system": "5.1.15",
    "@types/styled-system__css": "5.0.16",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-plugin-styled-components": "2.0.7",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.19.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-next": "12.2.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.2.4",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.1",
    "jest-junit": "14.0.0",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "ts-node": "10.8.2",
    "typescript": "4.7.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
